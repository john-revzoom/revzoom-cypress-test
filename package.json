    "export": "next export",
    "start": "next start",
    "lint": "next lint --no-cache",
    "prepare": "husky install",
    "pretty:check:src": "prettier --check src/**/*.ts*",
    "pretty:check": "prettier --check",
    "pretty:write": "prettier --write",
    "tsc": "tsc",
    "start:test": "set DOT_ENV=\"test\" && next start",
